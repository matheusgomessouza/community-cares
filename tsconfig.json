{
  "compilerOptions": {
    "baseUrl": "./src/",
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "module": "node16",
    "moduleResolution": "node16",
    "paths": {
      "@components/*": ["components/*"],
      "@services/*": ["services/*"],
      "@contexts/*": ["contexts/*"],
      "@hooks/*": ["hooks/*"],
      "@interfaces/*": ["interfaces/*"]
    }
  },
  "extends": "@tsconfig/react-native/tsconfig.json"
}

    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/montserrat": "^0.2.3",
    "@expo-google-fonts/shrikhand": "^0.2.3",
    "@types/react": "~18.2.45",
    "axios": "^1.6.7",
    "expo": "50.0.11",
    "expo-auth-session": "~5.4.0",
    "expo-constants": "~15.4.5",
    "expo-crypto": "12.8.1",
    "expo-font": "11.10.3",
    "expo-linear-gradient": "12.7.2",
    "expo-linking": "~6.2.2",
    "expo-router": "3.4.8",
    "expo-secure-store": "~12.8.1",
    "expo-status-bar": "~1.11.1",
    "expo-web-browser": "~12.8.2",
    "jest": "^29.3.1",
    "jest-expo": "50.0.3",
